{
  "identifier": "migrated",
  "description": "permissions that were migrated from v1",
  "local": true,
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    {
      "identifier": "fs:scope",
      "allow": [
        "$RESOURCE/*"
      ]
    },
    {
      "identifier": "http:default",
      "allow": [{ "url": "http://127.0.0.1:7272/init_model" }, { "url": "http://127.0.0.1:7272/run_detection" }]
    },
    "shell:allow-open",
    "fs:default",
    "os:allow-platform",
    {
      "identifier": "fs:allow-exists",
      "allow": [{ "path": "$APPLOCALDATA/*" }]
    },
    {
      "identifier": "fs:allow-write",
      "allow": [{ "path": "$APPLOCALDATA/*" }]
    },
    {
      "identifier": "fs:allow-read",
      "allow": [{ "path": "$APPLOCALDATA/*" }]
    },
    {
      "identifier": "fs:allow-remove",
      "allow": [{ "path": "$APPLOCALDATA/*" }]
    },
    {
      "identifier": "fs:allow-create",
      "allow": [{ "path": "$APPLOCALDATA/*" }]
    },
    "shell:default",
    "updater:default",
    "log:default"
  ]
}